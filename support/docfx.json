{
  "build": {

    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],

    "resource": [
      {
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],

    "overwrite": [],
    "externalReference": [],

    "globalMetadata": {
      "breadcrumb_path": "/support/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },

    "fileMetadata": {

      "layout":{
        "**/index.yml": "LandingPage"
      },

      "manager": {
        "**/*.md": "dcscontentpm"
      },
      "audience": {
        "**/*.md": "itpro"
      },
      "author": {
        "**/*.md": "ramakoni1"
      },
      "ms.author": {
        "**/*.md": "ramakoni"
      },
      "ms.localizationpriority": {
        "**/*.md": "Normal"
      },
      "ms.reviewer": {
        "**/*.md": "DEV_Triage, DaleChe, RamaKoni, DelHan, SureshKa, SueHar"
      },
      "ms.topic": {
        "**/*.md": "troubleshooting"
      },

      "ms.prod": {
        "biztalk/**/*.md": "biztalk-server"
      },
      "ms.technology": {
        "biztalk/**/*.md": ""
      },

      "titleSuffix": {
        "biztalk/**/*.md": "BizTalk Server"
      }

    },

    "template": [],
    "dest": "support1-docset",
    "markdownEngineName": "markdig"

  }
}
